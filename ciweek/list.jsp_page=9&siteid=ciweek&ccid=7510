<script type="text/javascript" src="/v7/js/gotop.js"></script>
<script type="text/javascript" src="http://v3.jiathis.com/code/jia.js" charset="utf-8"></script>
<script>
$(document).ready(function(){
	//con_right 高度自适应
	$(function($){
		/*var pageWidth = window.innerWidth;
		if (typeof pagewidth != "number"){
			if(document.compatMode == "CSS1Compat"){
				pagewidth = document.documentElement.clientWidth;	
			}else{
				pagewidth = document.body.clientWidth;	
			}	
		}*/
		var pageWidth = $(window).width();
		if( pageWidth > 767){
			var dlright = $('.dlright').height()+''.replace('px','')*1;
			var con_left = $('.con_left').height()+''.replace('px','')*1;
			var dtright = $('.dlright dt').height()+''.replace('px','')*1;
			var ddright = con_left - dtright-106;
			if( dlright > con_left){
				$('.dlright').css('height',dlright)
			}else{
				$('.dlright dd').css('height',ddright);
			}
		}	
	})
})
</script>
